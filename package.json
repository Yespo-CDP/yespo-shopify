{
  "name": "yespo-app",
  "private": true,
  "scripts": {
    "build": "remix vite:build",
    "dev": "shopify app dev",
    "config:link": "shopify app config link",
    "generate": "shopify app generate",
    "deploy": "shopify app deploy",
    "config:use": "shopify app config use",
    "env": "shopify app env",
    "start": "remix-serve ./build/server/index.js",
    "docker-start": "npm run setup && npm run start",
    "setup": "prisma generate && prisma migrate deploy",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "shopify": "shopify",
    "prisma": "prisma",
    "graphql-codegen": "graphql-codegen",
    "vite": "vite",
    "docs:html": "typedoc --options typedoc.html.json",
    "docs:md": "typedoc --options typedoc.md.json",
    "docs": "npm run docs:html && npm run docs:md",
    "worker": "npx tsx ./app/worker/worker.ts"
  },
  "type": "module",
  "engines": {
    "node": "^18.20 || ^20.10 || >=21.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "@remix-run/dev": "^2.16.2",
    "@remix-run/fs-routes": "^2.16.2",
    "@remix-run/node": "^2.16.2",
    "@remix-run/react": "^2.16.2",
    "@remix-run/serve": "^2.16.2",
    "@shopify/app-bridge-react": "^4.1.6",
    "@shopify/graphql-client": "^1.4.1",
    "@shopify/polaris": "^12.0.0",
    "@shopify/shopify-app-remix": "^3.7.0",
    "@shopify/shopify-app-session-storage-prisma": "^6.0.0",
    "@upstash/qstash": "^2.8.1",
    "bullmq": "^5.56.9",
    "comment-json": "^4.2.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "i18next": "^23.16.8",
    "isbot": "^5.1.0",
    "prisma": "^6.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.3",
    "remix-i18next": "^6.4.1",
    "uuid": "^11.1.0",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "devDependencies": {
    "@remix-run/eslint-config": "^2.16.2",
    "@remix-run/route-config": "^2.16.2",
    "@shopify/api-codegen-preset": "^1.1.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.16.0",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "docdash": "^2.0.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^10.0.1",
    "jsdoc": "^4.0.4",
    "jsdoc-to-markdown": "^9.1.1",
    "prettier": "^3.2.4",
    "typedoc": "^0.28.7",
    "typedoc-plugin-markdown": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^6.2.2"
  },
  "workspaces": [
    "extensions/*"
  ],
  "trustedDependencies": [
    "@shopify/plugin-cloudflare"
  ],
  "resolutions": {
    "@graphql-tools/url-loader": "8.0.16"
  },
  "overrides": {
    "@graphql-tools/url-loader": "8.0.16"
  },
  "author": "yespoplugin@yespo.io"
}
