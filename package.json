{
  "name": "yespo-app",
  "private": true,
  "scripts": {
    "build": "react-router build",
    "dev": "shopify app dev",
    "config:link": "shopify app config link",
    "generate": "shopify app generate",
    "deploy": "shopify app deploy",
    "config:use": "shopify app config use",
    "env": "shopify app env",
    "start": "react-router-serve ./build/server/index.js",
    "docker-start": "npm run setup && npm run start",
    "setup": "prisma generate && prisma migrate deploy",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "shopify": "shopify",
    "prisma": "prisma",
    "graphql-codegen": "graphql-codegen",
    "vite": "vite",
    "typecheck": "react-router typegen && tsc --noEmit",
    "docs:html": "typedoc --options typedoc.html.json",
    "docs:md": "typedoc --options typedoc.md.json",
    "docs": "npm run docs:html && npm run docs:md",
    "worker": "npx tsx ./app/worker/worker.ts"
  },
  "type": "module",
  "engines": {
    "node": ">=20.10"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@react-router/dev": "^7.9.3",
    "@react-router/fs-routes": "^7.9.3",
    "@react-router/node": "^7.9.3",
    "@react-router/serve": "^7.9.3",
    "@shopify/app-bridge-react": "^4.2.7",
    "@shopify/app-bridge-ui-types": "^0.1.1",
    "@shopify/graphql-client": "^1.4.1",
    "@shopify/polaris": "^12.27.0",
    "@shopify/shopify-app-react-router": "^1.0.0",
    "@shopify/shopify-app-session-storage-prisma": "^7.0.0",
    "@upstash/qstash": "^2.8.4",
    "bullmq": "^5.63.1",
    "comment-json": "^4.4.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "i18next": "^23.16.8",
    "isbot": "^5.1.32",
    "prisma": "^6.19.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.3",
    "react-router": "^7.9.3",
    "uuid": "^11.1.0",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "devDependencies": {
    "@shopify/api-codegen-preset": "^1.2.0",
    "@shopify/polaris-types": "^1.0.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.19.1",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "docdash": "^2.0.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "jsdoc": "^4.0.5",
    "jsdoc-to-markdown": "^9.1.3",
    "prettier": "^3.2.4",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "vite": "^6.4.1"
  },
  "workspaces": [
    "extensions/*"
  ],
  "trustedDependencies": [
    "@shopify/plugin-cloudflare"
  ],

  "author": "yespoplugin@yespo.io"
}
